{
    "name": "Rumkin.com",
    "private": true,
    "ava": {
        "files": [
            "site/**/*.spec.js"
        ]
    },
    "scripts": {
        "build": "node metalsmith.js",
        "clean": "rm -rf node_modules/ package-lock.json build",
        "fastbuild": "FAST=true node metalsmith.js",
        "reinstall": "rm -rf node_modules/ && npm install --legacy-peer-deps",
        "start": "SERVE=true LIVE_RELOAD=true node metalsmith.js",
        "test": "ava -v",
        "watch": "ava -v --watch"
    },
    "dependencies": {
        "@babel/preset-env": "^7.15.0",
        "@fidian/metalsmith-site": "^1.1.5",
        "@fidian/rumkin-cipher": "*",
        "@fidian/rumkin-compression": "^1.1.0",
        "browserify": "^17.0.0",
        "clean-css": "^5.1.5",
        "jquery": "^3.6.0",
        "marked": "^2.1.3",
        "metalsmith-babel": "^5.1.0",
        "metalsmith-brotli": "^2.0.0",
        "metalsmith-browserify-alt": "^2.0.0",
        "metalsmith-clean-css": "^6.1.3",
        "metalsmith-gzip": "^1.1.0",
        "metalsmith-html-minifier": "^4.0.1",
        "metalsmith-uglify": "^2.4.0",
        "mithril": "^2.0.4",
        "polyfill-crypto.getrandomvalues": "^1.0.0",
        "psr": "^0.0.1",
        "tablesorter": "^2.31.3",
        "tai-password-strength": "^1.1.2"
    }
}
